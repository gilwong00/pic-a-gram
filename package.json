		"express": "^4.17.1",
		"express-session": "^1.17.1",
		"graphql": "^15.4.0",
		"nodemon": "^2.0.4",
		"pg": "^8.4.0",
		"reflect-metadata": "^0.1.10",
		"type-graphql": "^1.1.1",
		"typeorm": "0.2.29"
	},
	"devDependencies": {
		"@types/bcryptjs": "^2.4.2",
		"@types/cors": "^2.8.8",
		"@types/express": "^4.17.9",
		"@types/express-session": "^1.17.3",
		"@types/node": "^8.0.29",
		"ts-node": "3.3.0",
		"ts-node-dev": "^1.0.0",
		"typescript": "3.3.3333"
